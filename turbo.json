{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["build/**"]
        },
        "test": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "format": {
            "outputs": []
        },
        "deploy": {
            "dependsOn": ["lint", "test", "build"],
            "outputs": []
        },
        "dev": {
            "cache": false
        }
    }
}
