{
    "name": "@jcakery/jcash",
    "version": "0.1.0",
    "author": "JCakery",
    "private": true,
    "workspaces": [
        "packages/**/*"
    ],
    "bin": {
        "jcash": "bin/jcash.js"
    },
    "engines": {
        "node": "^14.0.0",
        "yarn": "^1.22.0"
    },
    "scripts": {
        "build:cdk": "tsc",
        "build:cdk:watch": "tsc -w",
        "build:frontend": "yarn workspace @jcakery/jcash-frontend run build",
        "test:cdk": "jest lib",
        "test:frontend": "yarn workspace @jcakery/jcash-frontend run test",
        "test:backend": "yarn workspace @jcakery/jcash-lambda run test",
        "test:packages": "yarn workspaces run test",
        "lint": "eslint --ext ts,tsx lib bin packages",
        "format": "yarn lint --fix && prettier --write \"**/*.ts\" \"**/*.tsx\"",
        "synth:dev": "ENV=dev cdk synth --no-staging > template.yml",
        "synth:prod": "ENV=prod cdk synth --no-staging > template.yml",
        "deploy:dev": "ENV=dev cdk deploy",
        "deploy:prod": "ENV=prod cdk deploy",
        "destroy:dev": "ENV=dev cdk destroy",
        "destroy:prod": "ENV=prod cdk destroy",
        "prepare": "husky install"
    },
    "dependencies": {
        "@aws-cdk/aws-apigateway": "^1.120.0",
        "@aws-cdk/aws-certificatemanager": "^1.120.0",
        "@aws-cdk/aws-cloudfront": "^1.120.0",
        "@aws-cdk/aws-dynamodb": "^1.120.0",
        "@aws-cdk/aws-iam": "^1.120.0",
        "@aws-cdk/aws-lambda": "^1.120.0",
        "@aws-cdk/aws-lambda-nodejs": "^1.120.0",
        "@aws-cdk/aws-route53": "^1.120.0",
        "@aws-cdk/aws-route53-targets": "^1.120.0",
        "@aws-cdk/aws-s3": "^1.120.0",
        "@aws-cdk/aws-s3-deployment": "^1.120.0",
        "@aws-cdk/core": "^1.120.0",
        "@types/source-map-support": "^0.5.4",
        "source-map-support": "^0.5.16"
    },
    "devDependencies": {
        "@aws-cdk/assert": "^1.120.0",
        "@jest/types": "^27.1.0",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.7.4",
        "@typescript-eslint/eslint-plugin": "^4.29.2",
        "@typescript-eslint/parser": "^4.29.2",
        "aws-cdk": "^1.120.0",
        "esbuild": "^0.12.24",
        "eslint": "^7.18.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.25.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^7.0.0",
        "jest": "^27.1.0",
        "lint-staged": "^11.1.2",
        "prettier": "^2.2.1",
        "relay-compiler": "^12.0.0",
        "relay-compiler-language-typescript": "^14.1.0",
        "relay-config": "^12.0.0",
        "ts-jest": "^27.0.4",
        "ts-node": "^10.2.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "4.3.*"
    }
}
