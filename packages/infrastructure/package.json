{
    "name": "@app/infrastructure",
    "version": "1.0.0",
    "private": true,
    "description": "CDK Infrastructures for App",
    "dependencies": {
        "@aws-cdk/aws-apigateway": "^1.130.0",
        "@aws-cdk/aws-apigatewayv2": "^1.130.0",
        "@aws-cdk/aws-apigatewayv2-integrations": "^1.130.0",
        "@aws-cdk/aws-certificatemanager": "^1.130.0",
        "@aws-cdk/aws-cloudfront": "^1.130.0",
        "@aws-cdk/aws-dynamodb": "^1.130.0",
        "@aws-cdk/aws-ec2": "^1.127.0",
        "@aws-cdk/aws-iam": "^1.130.0",
        "@aws-cdk/aws-lambda": "^1.130.0",
        "@aws-cdk/aws-lambda-nodejs": "^1.130.0",
        "@aws-cdk/aws-rds": "^1.127.0",
        "@aws-cdk/aws-route53": "^1.130.0",
        "@aws-cdk/aws-route53-targets": "^1.130.0",
        "@aws-cdk/aws-s3": "^1.130.0",
        "@aws-cdk/aws-s3-deployment": "^1.130.0",
        "@aws-cdk/core": "^1.130.0",
        "@types/source-map-support": "^0.5.4",
        "source-map-support": "^0.5.16"
    },
    "devDependencies": {
        "@aws-cdk/assert": "^1.130.0",
        "aws-cdk": "^1.130.0"
    },
    "scripts": {
        "test": "jest",
        "lint": "eslint --ext ts lib bin",
        "format": "prettier --write \"**/*.ts\"",
        "synth:dev": "ENV=dev cdk synth --no-staging > template.yml",
        "synth:prod": "ENV=prod cdk synth --no-staging > template.yml",
        "deploy:dev": "ENV=dev cdk deploy",
        "deploy:prod": "ENV=prod cdk deploy",
        "destroy:dev": "ENV=dev cdk destroy",
        "destroy:prod": "ENV=prod cdk destroy"
    }
}
