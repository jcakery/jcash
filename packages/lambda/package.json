{
    "name": "@app/lambda",
    "version": "1.0.0",
    "private": true,
    "description": "App Lambdas",
    "dependencies": {
        "@aws-sdk/client-secrets-manager": "^3.37.0",
        "@prisma/client": "^3.0.2",
        "aws-lambda-fastify": "^1.7.1",
        "fastify": "^3.21.3",
        "fastify-cors": "^6.0.2",
        "graphql": "^15.5.3",
        "mercurius": "^8.2.1",
        "nexus": "^1.1.0",
        "nexus-plugin-prisma": "^0.35.0"
    },
    "devDependencies": {
        "prisma": "^3.0.2",
        "ts-node-dev": "^1.1.8"
    },
    "scripts": {
        "test": "jest",
        "lint": "eslint --ext ts .",
        "format": "prettier --write \"**/*.ts\" \"**/*.tsx\"",
        "start": "tsnd --poll --transpile-only --no-notify ./src/app.ts",
        "prisma:gen": "npx prisma generate",
        "prisma:migrate": "npx prisma migrate dev",
        "prisma:save": "npm run prisma:migrate -- --create-only",
        "nexus:gen": "ts-node src/graphql/index.ts",
        "build": "tsc"
    }
}
